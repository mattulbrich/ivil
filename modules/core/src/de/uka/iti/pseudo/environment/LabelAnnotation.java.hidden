package de.uka.iti.pseudo.environment;

public class LabelAnnotation extends ProgramAnnotation {

    private String label;

    public LabelAnnotation(String label, int statementNo) {
        super(statementNo);
        this.label = label;
    }

    public String toString() {
        return label + ":";
    }

    public String getLabel() {
        return label;
    }

}
